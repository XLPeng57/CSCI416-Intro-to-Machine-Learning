    "xv = []\n",
    "for i in range(iters):\n",
    "    xv.append(i)\n",
    "plt.plot(xv, cost)\n",
    "plt.xlabel(\"Iterations\")\n",
    "plt.ylabel(\"Cost\")\n",
    "plt.show()"
